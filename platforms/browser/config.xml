<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.gzsistemas.flashvenda" version="1.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>GZ Flash</name>
    <description>Aplicativo para acompanhar os valores de vendas em tempo real</description>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Pré Venda</name>
    <description>Aplicativo para lanças itens em comandas</description>
    <author email="suporte@gzcloud.com.br" href="http://gzcloud.com.br">GZ Sistemas - GZ Cloud</author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
